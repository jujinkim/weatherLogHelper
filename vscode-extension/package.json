{
  "name": "weather-log-helper",
  "displayName": "Weather Log Helper",
  "description": "Internal WLH client",
  "version": "0.0.2",
  "publisher": "jujinkim",
  "author": "jujin.kim",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/jujinkim/weatherLogHelper.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:wlh.scan",
    "onCommand:wlh.scanForce",
    "onCommand:wlh.decryptCurrentFile",
    "onCommand:wlh.openSettings",
    "onCommand:wlh.openEngineConfig",
    "onCommand:wlh.status",
    "onCommand:wlh.stop",
    "onCommand:wlh.openHome",
    "onCommand:wlh.refreshView",
    "onCommand:wlh.engineMenu",
    "onView:wlh.sidebar",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "versionInfo": "2025-01-14T22:33:00Z",
  "contributes": {
    "commands": [
      {
        "command": "wlh.scan",
        "title": "WLH: Scan"
      },
      {
        "command": "wlh.scanForce",
        "title": "WLH: Force Scan"
      },
      {
        "command": "wlh.decryptCurrentFile",
        "title": "WLH: Decrypt Current File"
      },
      {
        "command": "wlh.openSettings",
        "title": "WLH: Open Settings"
      },
      {
        "command": "wlh.openEngineConfig",
        "title": "WLH: Open Engine Config"
      },
      {
        "command": "wlh.status",
        "title": "WLH: Engine Status"
      },
      {
        "command": "wlh.stop",
        "title": "WLH: Engine Stop"
      },
      {
        "command": "wlh.openHome",
        "title": "WLH: Open WLH Home Folder"
      },
      {
        "command": "wlh.refreshView",
        "title": "WLH: Panel Refresh"
      },
      {
        "command": "wlh.engineMenu",
        "title": "WLH: Engine Menu"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "wlh.refreshView",
          "when": "view == wlh.sidebar",
          "group": "navigation@1",
          "icon": "$(refresh)"
        },
        {
          "command": "wlh.engineMenu",
          "when": "view == wlh.sidebar",
          "group": "navigation@2",
          "icon": "$(ellipsis)"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "wlh",
          "title": "WLH",
          "icon": "media/wlh.svg"
        }
      ]
    },
    "views": {
      "wlh": [
        {
          "id": "wlh.sidebar",
          "name": "WLH",
          "type": "webview"
        }
      ]
    },
    "configuration": {
      "title": "Weather Log Helper",
      "properties": {
        "wlh.update.baseUrl": {
          "type": "string",
          "default": "",
          "description": "Base URL for WLH engine updates"
        },
        "wlh.decrypt.jarPath": {
          "type": "string",
          "default": "",
          "description": "Path to decrypt jar"
        },
        "wlh.decrypt.timeoutSeconds": {
          "type": "number",
          "default": 10,
          "description": "Decrypt timeout in seconds"
        },
        "wlh.commandPath": {
          "type": "string",
          "default": "",
          "description": "Path to the wlh executable"
        }
      }
    }
  },
  "scripts": {
    "build": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/vscode": "^1.80.0",
    "typescript": "^5.4.5"
  }
}
